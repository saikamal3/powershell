function get-firstrecurringcharacter {
   [CmdletBinding()]
   param (
     [string]$teststring
   )
  $a = [ordered]@{}
  $b = [ordered]@{}
  $teststring.ToLower().ToCharArray() |
   foreach {
     if ($a.$psitem) {
        $a.$psitem += 1
        if(!$b.$psitem){
        $b += @{$psitem = 1}
     }
        
     }
     else {
       $a += @{$psitem = 1}
     }

  }

  $b.GetEnumerator() |  select -First 1

}
$ts = 'teststring'

get-firstrecurringcharacter -teststring $ts
